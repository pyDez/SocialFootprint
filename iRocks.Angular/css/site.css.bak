/* Arrows */
#prevSlide,
#nextSlide {
    font-size: 0;
    line-height: 0;
    position: absolute;
    top: 50%;
    display: block;
    width: 20px;
    height: 20px;
    margin-top: -10px;
    padding: 0;
    cursor: pointer;
    color: #fff;
    border: none;
    outline: none;
    background: #dd4814;
}

.slideArrow {
    font-size: 16px;
}

#prevSlide:hover,
#prevSlide:focus,
#nextSlide:hover,
#nextSlide:focus {
    color: #dd4814;
    outline: none;
    background: transparent;
}

    #prevSlide:hover:before,
    #prevSlide:focus:before,
    #nextSlide:hover:before,
    #nextSlide:focus:before {
        opacity: 1;
    }

#prevSlide.slick-disabled:before,
#nextSlide.slick-disabled:before {
    opacity: .25;
}

#prevSlide:before,
#nextSlide:before {
    font-family: 'slick';
    font-size: 20px;
    line-height: 1;
    opacity: .75;
    color: white;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

#prevSlide {
    left: -10px;
}

[dir='rtl'] #prevSlide {
    right: -10px;
    left: auto;
}

#nextSlide {
    right: -10px;
}

[dir='rtl'] #nextSlide {
    right: auto;
    left: -10px;
}
